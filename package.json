{
  "name": "task-manager-api",
  "version": "0.0.1",
  "description": "Task Manager API",
  "scripts": {
    "dev": "ts-node-dev --no-deps --respawn --poll --interval 3000 src/server.ts -p",
    "dev:windows": "set NODE_ENV=dev && ts-node-dev --no-deps --respawn --poll --interval 1000 src/server.ts -p",
    "test": "jest --silent --noStackTrace --runInBand",
    "test:ci": "export NODE_ENV=ci && npm test -- --coverage --detectOpenHandles",
    "test:local": "set NODE_ENV=test && npm test -- --coverage --detectOpenHandles",
    "lint": "npx eslint . --fix",
    "config-git": "git config --global core.autocrlf input",
    "build": "tsc"
  },
  "keywords": [
    "taskmanager",
    "task",
    "manager"
  ],
  "author": "Lucas Sahdo <lucassahdo@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@jest/globals": "^29.0.3",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/crypto-js": "^4.0.1",
    "@types/express": "^4.17.9",
    "@types/helmet": "^4.0.0",
    "@types/i18n": "^0.12.0",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/luxon": "^1.25.1",
    "@types/node": "^14.18.26",
    "@types/nodemailer": "^6.4.0",
    "@types/qrcode": "^1.4.0",
    "@types/react-router": "^5.1.14",
    "@types/speakeasy": "^2.0.5",
    "@types/superagent": "^4.1.10",
    "@types/supertest": "^2.0.10",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "@types/validator": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "3",
    "eslint": "7",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "eslint-plugin-standard": "4",
    "jest": "^29.0",
    "sequelize-cli": "^6.4.1",
    "supertest": "6.0.0",
    "ts-jest": "^29.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.1.3"
  },
