    "crypto-js": "^4.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.3.1",
    "http-status-codes": "^2.1.4",
    "i18n": "^0.13.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luhn": "^2.4.1",
    "luxon": "^1.25.0",
    "nodemailer": "^6.4.17",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
